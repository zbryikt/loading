svg.uil-pacman(width="100%",height="100%",viewBox="0 0 100 100",preserveAspectRatio="xMidYMid")
  rect.bk(x="0",y="0",width="100",height="100",fill="#ffffff")
  path(d="M0 50A50 50 0 1 0 100 50",fill="#000",transform="rotate(30 50 50)")
    animateTransform(attributeName="transform",type="rotate",dur="1s",repeatCount="indefinite",
    from="30 50 50",to="30 50 50",values="30 50 50;0 50 50;30 50 50")
  path(d="M0 50A50 50 0 1 1 100 50",fill="#000",transform="rotate(-30 50 50)")
    animateTransform(attributeName="transform",type="rotate",dur="1s",repeatCount="indefinite",
    from="-30 50 50",to="-30 50 50",values="-30 50 50;0 50 50;-30 50 50")
